{
  "id": "plugin.pot-app-tts-plugin-siliconflow",
  "version": "0.0.1",
  "plugin_type": "tts",
  "display": "Siliconflow Pot 文本转语音插件",
  "icon": "siliconflow.svg",
  "homepage": "https://github.com/whymeta/pot-app-tts-plugin-siliconflow",
